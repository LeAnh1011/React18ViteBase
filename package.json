    "@types/carbon__icons-react": "^11.16.0",
    "@types/file-saver": "^2.0.4",
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.171",
    "@types/node": "^12.0.0",
    "@types/react": "^18.2.6",
    "@types/react-collapse": "^5.0.1",
    "@types/react-dom": "^18.2.4",
    "@types/react-helmet": "^6.1.6",
    "@types/react-redux": "^7.1.25",
    "@types/react-router": "^5.1.8",
    "@types/react-router-dom": "^5.1.5",
    "@types/uuid": "^9.0.1",
    "@types/webfontloader": "^1.6.33",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "@vitejs/plugin-react": "^3.1.0",
    "babel-plugin-macros": "^3.1.0",
    "env-cmd": "^10.1.0",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^4.2.3",
    "lint-staged": "^12.3.7",
    "npm-run-all": "^4.1.5",
    "path-browserify": "^1.0.1",
    "prettier": "^2.1.1",
    "react-app-rewire-postcss": "^3.0.2",
    "react3l-cli": "^1.1.0",
    "rollup-plugin-visualizer": "^5.9.0",
    "ts-nameof.macro": "^4.2.1",
    "vite": "4.4.9",
    "vite-plugin-ts-nameof": "^1.2.1"
  },
  "scripts": {
    "serve": "vite preview --outDir build",
    "prestart": "run-s merge",
    "start": "vite",
    "prebuild": "run-s merge",
    "build": "tsc && vite build",
    "build:staging": "yarn build -- --mode staging",
    "merge": "react3l translate merge -i src/ -o src/locales/ -p src/locales/",
    "extract": "react3l translate extract -i src/ -o src/locales/ -p src/locales/",
    "lint": "eslint --fix --ext .js,.jsx,.ts,.tsx src",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md,html,css,less,scss}\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.+(ts|tsx|js|jsx|json|md|html|css|less|scss)": [
      "prettier --write"
    ],
    "src/**/*.+(js|jsx|ts|tsx)": [
      "eslint --ext .js,.jsx,.ts,.tsx"
    ]
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:import/recommended",
      "plugin:jsx-a11y/recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:react-hooks/recommended",
      "eslint-config-prettier"
    ],
    "settings": {
      "react": {
        "version": "detect"
      },
      "import/resolver": {
        "node": {
          "paths": [
            "src"
          ],
          "extensions": [
            ".js",
            ".jsx",
            ".ts",
            ".tsx"
          ]
        }
      }
    },
    "rules": {
      "no-console": "warn",
      "no-debugger": "error",
      "react/jsx-uses-react": "off",
      "react/react-in-jsx-scope": "off",
      "react/jsx-no-target-blank": "off",
      "react/prop-types": "off",
      "jsx-a11y/click-events-have-key-events": "off",
      "jsx-a11y/no-static-element-interactions": "off"
    },
    "overrides": [
      {
        "files": [
          "*.ts",
          "*.tsx"
        ],
        "rules": {
          "no-undef": "off"
        }
      }
    ],
    "ignorePatterns": [
      "src/**/*.test.{ts,tsx}",
      "**/*.{scss,css}"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
